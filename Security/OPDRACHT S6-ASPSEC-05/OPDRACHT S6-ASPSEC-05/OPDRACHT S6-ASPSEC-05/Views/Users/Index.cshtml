@using Microsoft.AspNetCore.Identity
@model IEnumerable<IdentityUser>
<h2>Gebruikersbeheer</h2>
<table>
    <tr>
        <th>Email</th>
        <th>Acties</th>
    </tr>
    @foreach (var user in Model)
    {
        <tr>
            <td>@user.Email</td>
            <td>
                <a href="#">Verwijderen</a>
            </td>
        </tr>
    }
</table>
